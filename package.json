{
  "name": "showstop-chatbot",
  "version": "1.0.0",
  "description": "ShowStop ChatBot - Marketing Data Analytics with AI-Powered SQL Translation",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "build:prod": "npm install && cd client && npm install && npm run build",
    "start:prod": "NODE_ENV=production node server/index.js",
    "install-all": "npm install && cd client && npm install",
    "setup-db": "node scripts/setup-database.js",
    "test-auth": "node scripts/generate-password.js",
    "test-server": "node scripts/test-server.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "chatbot",
    "marketing",
    "analytics",
    "ai",
    "sql",
    "dashboard",
    "meta-ads",
    "tiktok-ads",
    "google-ads"
  ],
  "author": "ShowStop Team",
  "license": "MIT"
}
